                <div id="updateUser" class="mainDiv" > <!-- style="display:none" -->
                    <div class="card-header" id="headerUser">
                            <h3 class="mb-0"> <a href="#"><a><img src="../img/reload.svg" class="svg icon header-icon"></a> Update User</h3>
                        </div>
                        <div class="card-body" id="cardBodyUser">
                            <!--  -->
                            <!--  -->
                            
                            <div class="form-group row">
                                <label class="col-lg-3 col-form-label form-control-label">Find</label>
                                <div class="col-lg-9">
                                    <div class="input-group form-group">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-search"></i></span>
                                            </div>
                                            <select id="findUserUpdate" data-placeholder="Begin typing a name to filter..."
                                                class="form-control chosen-select" tabindex="4" size="0" name="updateUser"
                                                required>
                                                {{#if users}}
                                                    <option disabled selected value> Select users from {{users.length}} </option>  
                                                    {{#each users}}
                                                   <option value="{{json this}}">{{this.username}}</option> 
                                                   {{/each}} 
                                                {{else}}
                                                <option disabled selected value> NO users available </option>  
                                                {{/if}}                                              
                                            </select>
                                        </div>                                 

                                </div>
                            </div>
                           
                            <!--  -->
                            <!--  -->
                            <form class="form" id="updateUserForm" role="form" autocomplete="off" action="http://127.0.0.1:3000/update"
                                method="POST" oninput='confirmPassword.setCustomValidity(confirmPassword.value != password.value ? "Passwords do not match." : "")'>  
                                <!-- ID Of user. It can be only read  -->
